{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "src/components/*"
      ],
      "@markdown/*": [
        "src/components/markdown/*"
      ],
      "@assets/*": [
        "src/assets/*"
      ],
      "@sections/*": [
        "src/sections/*"
      ],
    }
  }
}